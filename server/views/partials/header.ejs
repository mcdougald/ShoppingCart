<body>
<!-- header -->
<header class="header-top-bar" role="banner">
    <div class="header-text"><h1>Store Name</h1></div>
    <nav class="top-bar" role="navigation">
        <div class="top-bar responsive-links">
            <div class="top-bar-column-1">
                <ul class="nav-list">
                    <li class="nav-item" id="home-tab-button">
                        <a
                                class="nav-link active"
                                href="#"
                                id="Home-link"
                                onclick="openTab('Home')"
                        >Home</a
                        >
                    </li>
                    <li class="nav-item" id="shopping-tab-button">
                        <a
                                class="nav-link"
                                href="#"
                                id="Shop-link"
                                onclick="openTab('Shop')"
                        >Shop</a
                        >
                    </li>
                    <li class="nav-item" id="checkout-tab-button">
                        <a
                                class="nav-link"
                                href="#"
                                id="Checkout-link"
                                onclick="openTab('Checkout')"
                        >Checkout</a
                        >
                    </li>
                </ul>
            </div>
            <div id="registered-top-bar-column-2" class="top-bar-column-2">
                <ul class="nav-list">
                    <li class="nav-item" id="account-information-tab-button">
                        <a
                                class="nav-link"
                                href="#"
                                id="Account-link"
                                onclick="openTab('Account')"
                        >Account</a
                        >
                    </li>
                    <li class="nav-item" id="logout-button">
                        <a
                                class="nav-link"
